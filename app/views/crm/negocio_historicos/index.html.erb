<h1 id="header">negocio_historicos</h1>
<div class="acessos">
  <%= link_to 'Nuevo', new_negocio_historico_path, :class => "button add" %>
</div>

<table class="head_grid">
  <tr>
            <td>Negocio</t>
            <td>Etapa</t>
            <td>Tarefa</t>
            <td>Tipo tarefa</t>
      </tr>
</table>

<div class="rolagem" dir="div">
  <table>
    <% @negocio_historicos.each do |negocio_historico| %>
      <tr>
                  <td><%= negocio_historico.negocio_id %></td>
                  <td><%= negocio_historico.etapa_id %></td>
                  <td><%= negocio_historico.tarefa_id %></td>
                  <td><%= negocio_historico.tipo_tarefa_id %></td>
                <td><%= link_to 'Editar', edit_negocio_historico_path(negocio_historico) %></td>
        <td><%= link_to 'Borrar', negocio_historico, method: :delete, data: { confirm: 'Esta Seguro?' } %></td>
      </tr>
    <% end %>
  </table>
</div>

