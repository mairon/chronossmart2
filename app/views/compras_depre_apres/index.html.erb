<h1 id="header">compras_depre_apres</h1>
<div class="acessos">
  <%= link_to t('NEW'), new_compras_depre_apre_path, :class => "button add" %>
</div>

<table class="head_grid">
  <tr>
            <td><%= t('PURCHASE') %></td>
            <td>Anos</td>
            <td>Moeda</td>
            <td>Depre us</td>
            <td>Depre gs</td>
            <td>Depre rs</td>
            <td>Apre us</td>
            <td>Apre gs</td>
            <td>Apre rs</td>
            <td>Compras produto</td>
            <td><%= t('DATE') %></td>
      </tr>
</table>

<div class="rolagem" dir="div">
  <table>
    <% @compras_depre_apres.each do |compras_depre_apre| %>
      <tr>
                  <td><%= compras_depre_apre.compra_id %></td>
                  <td><%= compras_depre_apre.anos %></td>
                  <td><%= compras_depre_apre.moeda %></td>
                  <td><%= compras_depre_apre.depre_us %></td>
                  <td><%= compras_depre_apre.depre_gs %></td>
                  <td><%= compras_depre_apre.depre_rs %></td>
                  <td><%= compras_depre_apre.apre_us %></td>
                  <td><%= compras_depre_apre.apre_gs %></td>
                  <td><%= compras_depre_apre.apre_rs %></td>
                  <td><%= compras_depre_apre.compras_produto_id %></td>
                  <td><%= compras_depre_apre.data %></td>
                <td><%= link_to 'Editar', edit_compras_depre_apre_path(compras_depre_apre) %></td>
        <td><%= link_to t('DELETE'), compras_depre_apre, method: :delete, data: { confirm: 'Esta Seguro?' } %></td>
      </tr>
    <% end %>
  </table>
</div>

