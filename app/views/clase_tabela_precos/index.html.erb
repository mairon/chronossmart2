<h1 id="header">clase_tabela_precos</h1>
<div class="acessos">
  <%= link_to t('NEW'), new_clase_tabela_preco_path, :class => "button add" %>
</div>

<table class="head_grid">
  <tr>
            <td>Clase</td>
            <td>Tabela preco</td>
            <td>Perc</td>
      </tr>
</table>

<div class="rolagem" dir="div">
  <table>
    <% @clase_tabela_precos.each do |clase_tabela_preco| %>
      <tr>
                  <td><%= clase_tabela_preco.clase_id %></td>
                  <td><%= clase_tabela_preco.tabela_preco_id %></td>
                  <td><%= clase_tabela_preco.perc %></td>
                <td><%= link_to 'Editar', edit_clase_tabela_preco_path(clase_tabela_preco) %></td>
        <td><%= link_to t('DELETE'), clase_tabela_preco, method: :delete, data: { confirm: 'Esta Seguro?' } %></td>
      </tr>
    <% end %>
  </table>
</div>

