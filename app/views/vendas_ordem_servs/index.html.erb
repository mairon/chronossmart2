<h1 id="header">vendas_ordem_servs</h1>
<div class="acessos">
  <%= link_to t('NEW'), new_vendas_ordem_serv_path, :class => "button add" %>
</div>

<table class="head_grid">
  <tr>
            <td>Venda</td>
            <td>Ordem serv</td>
      </tr>
</table>

<div class="rolagem" dir="div">
  <table>
    <% @vendas_ordem_servs.each do |vendas_ordem_serv| %>
      <tr>
                  <td><%= vendas_ordem_serv.venda_id %></td>
                  <td><%= vendas_ordem_serv.ordem_serv_id %></td>
                <td><%= link_to 'Editar', edit_vendas_ordem_serv_path(vendas_ordem_serv) %></td>
        <td><%= link_to t('DELETE'), vendas_ordem_serv, method: :delete, data: { confirm: 'Esta Seguro?' } %></td>
      </tr>
    <% end %>
  </table>
</div>

