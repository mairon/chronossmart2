<h1 id="header">ordem_entrega_produtos</h1>
<div class="acessos">
  <%= link_to 'Nuevo', new_ordem_entrega_produto_path, :class => "button add" %>
</div>

<table class="head_grid">
  <tr>
            <td>Ordem entrega</t>
            <td>Produto</t>
            <td>Quantidade</t>
      </tr>
</table>

<div class="rolagem" dir="div">
  <table>
    <% @ordem_entrega_produtos.each do |ordem_entrega_produto| %>
      <tr>
                  <td><%= ordem_entrega_produto.ordem_entrega_id %></td>
                  <td><%= ordem_entrega_produto.produto_id %></td>
                  <td><%= ordem_entrega_produto.quantidade %></td>
                <td><%= link_to 'Editar', edit_ordem_entrega_produto_path(ordem_entrega_produto) %></td>
        <td><%= link_to 'Borrar', ordem_entrega_produto, method: :delete, data: { confirm: 'Esta Seguro?' } %></td>
      </tr>
    <% end %>
  </table>
</div>

