<h1 id="header">vendas_pedidos</h1>
<div class="acessos">
  <%= link_to 'Nuevo', new_vendas_pedido_path, :class => "button add" %>
</div>

<table class="head_grid">
  <tr>
            <td>Venda</t>
            <td>Presupuesto</t>
      </tr>
</table>

<div class="rolagem" dir="div">
  <table>
    <% @vendas_pedidos.each do |vendas_pedido| %>
      <tr>
                  <td><%= vendas_pedido.venda_id %></td>
                  <td><%= vendas_pedido.presupuesto_id %></td>
                <td><%= link_to 'Editar', edit_vendas_pedido_path(vendas_pedido) %></td>
        <td><%= link_to 'Borrar', vendas_pedido, method: :delete, data: { confirm: 'Esta Seguro?' } %></td>
      </tr>
    <% end %>
  </table>
</div>

