<h1 id="header">Retenciones</h1>
<div class="acessos">
  <%= link_to t('NEW'), new_retencao_path, :class => "button add" %>
</div>

<table class="head_grid">
  <tr>
    <td width="80">Lanz</td>
    <td width="80"><%= t('DATE') %></td>
    <td width="250">Proveedor</td>
  </tr>
</table>

<div class="rolagem" dir="div">
  <table>
    <% @retencaos.each do |retencao| %>
      <tr class = "<%= cycle("cor1", "cor2") %>">
        <td width="80"><%= retencao.id.to_s.rjust(6,'0') %></td>
        <td width="80"><%= l retencao.data, :format => :default %></td>
        <td width="250"><%= retencao.persona.nome %></td>
        <td><%= link_to 'Editar', edit_retencao_path(retencao) %></td>
        <td><%= link_to t('DELETE'), retencao, method: :delete, data: { confirm: 'Esta Seguro?' } %></td>
      </tr>
    <% end %>
  </table>
</div>

