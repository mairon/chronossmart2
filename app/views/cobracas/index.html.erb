<h1 id="header">cobracas</h1>
<div class="acessos">
  <%= link_to t('NEW'), new_cobraca_path, :class => "button add" %>
</div>

<table class="head_grid">
  <tr>
            <td><%= t('DATE') %></td>
            <td><%= t('EXPIRATION') %></td>
            <td>Responsavel</td>
      </tr>
</table>

<div class="rolagem" dir="div">
  <table>
    <% @cobracas.each do |cobraca| %>
      <tr>
                  <td><%= cobraca.data %></td>
                  <td><%= cobraca.vencimento %></td>
                  <td><%= cobraca.responsavel_id %></td>
                <td><%= link_to 'Editar', edit_cobraca_path(cobraca) %></td>
        <td><%= link_to t('DELETE'), cobraca, method: :delete, data: { confirm: 'Esta Seguro?' } %></td>
      </tr>
    <% end %>
  </table>
</div>

