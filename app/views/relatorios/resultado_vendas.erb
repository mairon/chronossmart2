<% if params[:detalhe] == "2" %>
  <%= render :partial => 'relatorios/forms/vendas/venda_por_produto' %>
<% elsif params[:detalhe] == "3" %>
	<%= render :partial => 'relatorios/forms/vendas/venda_por_bico' %>
<% elsif params[:detalhe] == "4" %>
	<%= render :partial => 'relatorios/forms/vendas/venda_vendedor_cliente' %>
<% elsif params[:detalhe] == "5" %>
	<%= render :partial => 'relatorios/forms/vendas/venda_resumo_vendedor' %>
<% elsif params[:detalhe] == "6" %>
	<%= render :partial => 'relatorios/forms/vendas/venda_vendedor_produto' %>
<% elsif params[:detalhe] == "7" %>
	<%= render :partial => 'relatorios/forms/vendas/venda_vendedor_agrupado_cliente' %>

<% else %>
  <%= render :partial => 'relatorios/forms/vendas/venda_resumido_e_detalhado' %>
<% end %>
