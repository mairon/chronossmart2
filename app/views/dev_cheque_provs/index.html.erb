<h1 id="header">Devoluci√≥n de Cheque Proveedor</h1>
<div class="acessos">
  <%= link_to t('NEW'), new_dev_cheque_prov_path, :class => "button add" %>
</div>

<table class="head_grid">
  <tr>
    <td width="50">Lanz.</td>
    <td width="70"><%= t('DATE') %></td>
    <td width="250">Cliente</td>
    <td width="200"><%= t('ACCOUNT') %></td>
    <td width="250">Concepto</td>
  </tr>
</table>

<div class="rolagem" dir="div">
  <table>
    <% @dev_cheque_provs.each do |dev_cheque_prov| %>
      <tr class = "<%= cycle("cor1", "cor2") %>">
        <td width="50"><%= dev_cheque_prov.id.to_s.rjust(6,'0') %></td>
        <td width="70"><%= dev_cheque_prov.data.to_date.strftime("%d/%m/%y") %></td>
        <td width="250"><%= dev_cheque_prov.persona.nome %></td>
        <td width="200"><%= dev_cheque_prov.conta.nome %></td>
        <td width="250"><%= dev_cheque_prov.obs %></td>
        <td><%= link_to 'Editar', edit_dev_cheque_prov_path(dev_cheque_prov) %></td>
        <td><%= link_to t('DELETE'), dev_cheque_prov, method: :delete, data: { confirm: 'Esta Seguro?' } %></td>
      </tr>
    <% end %>
  </table>
</div>

