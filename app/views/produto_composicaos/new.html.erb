<h1>New produto_composicao</h1>

<% form_for(@produto_composicao) do |f| %>
  <%= f.error_messages %>

  <p>
    <%= f.label :produto_id %><br />
    <%= f.text_field :produto_id %>
  </p>
  <p>
    <%= f.label :componente_id %><br />
    <%= f.text_field :componente_id %>
  </p>
  <p>
    <%= f.label :componente_nome %><br />
    <%= f.text_field :componente_nome %>
  </p>
  <p>
    <%= f.label :clase_id %><br />
    <%= f.text_field :clase_id %>
  </p>
  <p>
    <%= f.label :grupo_id %><br />
    <%= f.text_field :grupo_id %>
  </p>
  <p>
    <%= f.label :quantidade %><br />
    <%= f.text_field :quantidade %>
  </p>
  <p>
    <%= f.label :ultimo_custo %><br />
    <%= f.text_field :ultimo_custo %>
  </p>
  <p>
    <%= f.submit 'Create' %>
