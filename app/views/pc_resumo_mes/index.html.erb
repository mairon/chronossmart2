<h1 id="header">pc_resumo_mes</h1>
<div class="acessos">
  <%= link_to 'Nuevo', new_pc_resumo_me_path, :class => "button add" %>
</div>

<table class="head_grid">
  <tr>
            <td>Data</t>
            <td>Custo fixo gs</t>
            <td>Custo variavel gs</t>
            <td>Faturamento</t>
      </tr>
</table>

<div class="rolagem" dir="div">
  <table>
    <% @pc_resumo_mes.each do |pc_resumo_me| %>
      <tr>
                  <td><%= pc_resumo_me.data %></td>
                  <td><%= pc_resumo_me.custo_fixo_gs %></td>
                  <td><%= pc_resumo_me.custo_variavel_gs %></td>
                  <td><%= pc_resumo_me.faturamento %></td>
                <td><%= link_to 'Editar', edit_pc_resumo_me_path(pc_resumo_me) %></td>
        <td><%= link_to 'Borrar', pc_resumo_me, method: :delete, data: { confirm: 'Esta Seguro?' } %></td>
      </tr>
    <% end %>
  </table>
</div>

