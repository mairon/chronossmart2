<h1 id="header">venda_romaneios</h1>
<div class="acessos">
  <%= link_to 'Nuevo', new_venda_romaneio_path, :class => "button add" %>
</div>

<table class="head_grid">
  <tr>
            <td>Venda</t>
            <td>Romaneio</t>
      </tr>
</table>

<div class="rolagem" dir="div">
  <table>
    <% @venda_romaneios.each do |venda_romaneio| %>
      <tr>
                  <td><%= venda_romaneio.venda_id %></td>
                  <td><%= venda_romaneio.romaneio_id %></td>
                <td><%= link_to 'Editar', edit_venda_romaneio_path(venda_romaneio) %></td>
        <td><%= link_to 'Borrar', venda_romaneio, method: :delete, data: { confirm: 'Esta Seguro?' } %></td>
      </tr>
    <% end %>
  </table>
</div>

