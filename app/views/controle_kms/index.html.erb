<h1 id="header">controle_kms</h1>
<div class="acessos">
  <%= link_to t('NEW'), new_controle_km_path, :class => "button add" %>
</div>

<table class="head_grid">
  <tr>
            <td>Rodado</td>
            <td>Km</td>
            <td>Tabela</td>
            <td>Tabela</td>
            <td>Cod proc</td>
            <td>Sigla proc</td>
            <td><%= t('DATE') %></td>
      </tr>
</table>

<div class="rolagem" dir="div">
  <table>
    <% @controle_kms.each do |controle_km| %>
      <tr>
                  <td><%= controle_km.rodado_id %></td>
                  <td><%= controle_km.km %></td>
                  <td><%= controle_km.tabela %></td>
                  <td><%= controle_km.tabela_id %></td>
                  <td><%= controle_km.cod_proc %></td>
                  <td><%= controle_km.sigla_proc %></td>
                  <td><%= controle_km.data %></td>
                <td><%= link_to 'Editar', edit_controle_km_path(controle_km) %></td>
        <td><%= link_to t('DELETE'), controle_km, method: :delete, data: { confirm: 'Esta Seguro?' } %></td>
      </tr>
    <% end %>
  </table>
</div>

