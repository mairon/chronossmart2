<h1 id="header">config_contabils</h1>
<div class="acessos">
  <%= link_to t('NEW'), new_config_contabil_path, :class => "button add" %>
</div>

<table class="head_grid">
  <tr>
    <td width="200">Tabla</td>
    <td width="200">Cod contabil</td>
    <td width="300">Descricao</td>
  </tr>
</table>

<div class="rolagem" dir="div">
  <table>
    <% @config_contabils.each do |config_contabil| %>
      <tr class = "<%= cycle("cor1", "cor2") %>">
        <td width="200"><%= config_contabil.tabela %></td>
        <td width="200"><%= config_contabil.rubro_codigo %></td>
        <td width="300"><%= config_contabil.rubro_nome %></td>
        <td><%= link_to 'Editar', edit_config_contabil_path(config_contabil) %></td>
        <td><%= link_to t('DELETE'), config_contabil, method: :delete, data: { confirm: 'Esta Seguro?' } %></td>
      </tr>
    <% end %>
  </table>
</div>

