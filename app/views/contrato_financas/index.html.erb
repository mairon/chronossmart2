<h1 id="header">contrato_financas</h1>
<div class="acessos">
  <%= link_to t('NEW'), new_contrato_financa_path, :class => "button add" %>
</div>

<table class="head_grid">
  <tr>
            <td>Contrato</td>
            <td>Cota</td>
            <td><%= t('EXPIRATION') %></td>
            <td>Valor us</td>
            <td>Valor gs</td>
      </tr>
</table>

<div class="rolagem" dir="div">
  <table>
    <% @contrato_financas.each do |contrato_financa| %>
      <tr>
                  <td><%= contrato_financa.contrato %></td>
                  <td><%= contrato_financa.cota %></td>
                  <td><%= contrato_financa.vencimento %></td>
                  <td><%= contrato_financa.valor_us %></td>
                  <td><%= contrato_financa.valor_gs %></td>
                <td><%= link_to 'Editar', edit_contrato_financa_path(contrato_financa) %></td>
        <td><%= link_to t('DELETE'), contrato_financa, method: :delete, data: { confirm: 'Esta Seguro?' } %></td>
      </tr>
    <% end %>
  </table>
</div>

