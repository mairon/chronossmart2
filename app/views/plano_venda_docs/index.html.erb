<h1 id="header">plano_venda_docs</h1>
<div class="acessos">
  <%= link_to 'Nuevo', new_plano_venda_doc_path, :class => "button add" %>
</div>

<table class="head_grid">
  <tr>
            <td>Nome</t>
            <td>Obs</t>
      </tr>
</table>

<div class="rolagem" dir="div">
  <table>
    <% @plano_venda_docs.each do |plano_venda_doc| %>
      <tr>
                  <td><%= plano_venda_doc.nome %></td>
                  <td><%= plano_venda_doc.obs %></td>
                <td><%= link_to 'Editar', edit_plano_venda_doc_path(plano_venda_doc) %></td>
        <td><%= link_to 'Borrar', plano_venda_doc, method: :delete, data: { confirm: 'Esta Seguro?' } %></td>
      </tr>
    <% end %>
  </table>
</div>

