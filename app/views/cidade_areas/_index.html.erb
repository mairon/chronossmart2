<h1 id="header">cidade_areas</h1>
<div class="acessos">
  <%= link_to t('NEW'), new_cidade_area_path, :class => "button add" %>
</div>

<table class="head_grid">
  <tr>
            <td>Cidade</td>
            <td><%= t('NAME') %></td>
      </tr>
</table>

<div class="rolagem" dir="div">
  <table>
    <% @cidade_areas.each do |cidade_area| %>
      <tr>
                  <td><%= cidade_area.cidade_id %></td>
                  <td><%= cidade_area.nome %></td>
                <td><%= link_to 'Editar', edit_cidade_area_path(cidade_area) %></td>
        <td><%= link_to t('DELETE'), cidade_area, method: :delete, data: { confirm: 'Esta Seguro?' } %></td>
      </tr>
    <% end %>
  </table>
</div>

