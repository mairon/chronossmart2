<h1 id="header">personas_tabela_precos</h1>
<div class="acessos">
  <%= link_to t('NEW'), new_personas_tabela_preco_path, :class => "button add" %>
</div>

<table class="head_grid">
  <tr>
            <td>Persona</td>
            <td>Tabela preco</td>
      </tr>
</table>

<div class="rolagem" dir="div">
  <table>
    <% @personas_tabela_precos.each do |personas_tabela_preco| %>
      <tr>
                  <td><%= personas_tabela_preco.persona_id %></td>
                  <td><%= personas_tabela_preco.tabela_preco_id %></td>
                <td><%= link_to 'Editar', edit_personas_tabela_preco_path(personas_tabela_preco) %></td>
        <td><%= link_to t('DELETE'), personas_tabela_preco, method: :delete, data: { confirm: 'Esta Seguro?' } %></td>
      </tr>
    <% end %>
  </table>
</div>

