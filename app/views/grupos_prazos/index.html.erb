<h1 id="header">grupos_prazos</h1>
<div class="acessos">
  <%= link_to t('NEW'), new_grupos_prazo_path, :class => "button add" %>
</div>

<table class="head_grid">
  <tr>
            <td>Persona</td>
            <td>Grupo</td>
            <td>Prazo</td>
      </tr>
</table>

<div class="rolagem" dir="div">
  <table>
    <% @grupos_prazos.each do |grupos_prazo| %>
      <tr>
                  <td><%= grupos_prazo.persona_id %></td>
                  <td><%= grupos_prazo.grupo_id %></td>
                  <td><%= grupos_prazo.prazo_id %></td>
                <td><%= link_to 'Editar', edit_grupos_prazo_path(grupos_prazo) %></td>
        <td><%= link_to t('DELETE'), grupos_prazo, method: :delete, data: { confirm: 'Esta Seguro?' } %></td>
      </tr>
    <% end %>
  </table>
</div>

