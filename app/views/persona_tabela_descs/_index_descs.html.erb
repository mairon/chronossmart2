<table class="head_grid">
  <tr>
    <td width="70" align="center" ><%= t('DATE') %></td>
    <td width="300">Analisis</td>
    <td width="100" align="right">Desc us</td>
    <td width="100" align="right">Desc gs</td>
    <td width="100" align="right">Desc rs</td>
  </tr>
</table>

<div class="rolagem" dir="div">
  <table>
    <% persona_tabela_descs.each do |persona_tabela_desc| %>
      <tr class="<%= cycle("cor1", "cor2") %>" >
        <td width="70" align="center"><%= persona_tabela_desc.data.strftime("%d/%m/%y") %></td>
        <td width="300"><%= persona_tabela_desc.conj_ensaio.conjunto %></td>
        <td width="100" align="right"><%= number_to_currency(persona_tabela_desc.desc_us, :format=>' %n ', :separeitor => ",", :precision => 2) %></td>
        <td width="100" align="right"><%= number_to_currency(persona_tabela_desc.desc_gs, :format=>' %n ', :precision => 0) %></td>
        <td width="100" align="right"><%= number_to_currency(persona_tabela_desc.desc_rs, :format=>' %n ', :separeitor => ",", :precision => 2) %></td>
        <td><%= link_to 'Editar', edit_persona_tabela_desc_path(persona_tabela_desc) %></td>
        <td><%= link_to t('DELETE'), persona_tabela_desc, method: :delete, data: { confirm: 'Esta Seguro?' } %></td>
      </tr>
    <% end %>
  </table>
</div>

