<h1 id="header">produto_composicaos_vendas_produtos</h1>
<div class="acessos">
  <%= link_to 'Nuevo', new_produto_composicaos_vendas_produto_path, :class => "button add" %>
</div>

<table class="head_grid">
  <tr>
            <td>Produto composicao</t>
            <td>Vendas produto</t>
      </tr>
</table>

<div class="rolagem" dir="div">
  <table>
    <% @produto_composicaos_vendas_produtos.each do |produto_composicaos_vendas_produto| %>
      <tr>
                  <td><%= produto_composicaos_vendas_produto.produto_composicao_id %></td>
                  <td><%= produto_composicaos_vendas_produto.vendas_produto_id %></td>
                <td><%= link_to 'Editar', edit_produto_composicaos_vendas_produto_path(produto_composicaos_vendas_produto) %></td>
        <td><%= link_to 'Borrar', produto_composicaos_vendas_produto, method: :delete, data: { confirm: 'Esta Seguro?' } %></td>
      </tr>
    <% end %>
  </table>
</div>

