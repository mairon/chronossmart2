<h1 id="header">produto_imagens</h1>
<div class="acessos">
  <%= link_to 'Nuevo', new_produto_imagen_path, :class => "button add" %>
</div>

<table class="head_grid">
  <tr>
            <td>Produto</t>
            <td>Status</t>
      </tr>
</table>

<div class="rolagem" dir="div">
  <table>
    <% @produto_imagens.each do |produto_imagen| %>
      <tr>
                  <td><%= produto_imagen.produto_id %></td>
                  <td><%= produto_imagen.status %></td>
                <td><%= link_to 'Editar', edit_produto_imagen_path(produto_imagen) %></td>
        <td><%= link_to 'Borrar', produto_imagen, method: :delete, data: { confirm: 'Esta Seguro?' } %></td>
      </tr>
    <% end %>
  </table>
</div>

