<h1 id="header">ordem_serv_prods</h1>
<div class="acessos">
  <%= link_to t('NEW'), new_ordem_serv_prod_path, :class => "button add" %>
</div>

<table class="head_grid">
  <tr>
            <td>Ordem serv</td>
            <td>Deposito</td>
            <td>Produto</td>
            <td>Quantidade</td>
      </tr>
</table>

<div class="rolagem" dir="div">
  <table>
    <% @ordem_serv_prods.each do |ordem_serv_prod| %>
      <tr>
                  <td><%= ordem_serv_prod.ordem_serv %></td>
                  <td><%= ordem_serv_prod.deposito %></td>
                  <td><%= ordem_serv_prod.produto %></td>
                  <td><%= ordem_serv_prod.quantidade %></td>
                <td><%= link_to 'Editar', edit_ordem_serv_prod_path(ordem_serv_prod) %></td>
        <td><%= link_to t('DELETE'), ordem_serv_prod, method: :delete, data: { confirm: 'Esta Seguro?' } %></td>
      </tr>
    <% end %>
  </table>
</div>

