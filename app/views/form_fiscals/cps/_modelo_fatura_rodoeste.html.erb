													<%= I18n.with_locale(:es) { (tottaxa10_us.to_f + tottaxa0_us.to_f + tottaxa05_us.to_f).to_i.to_words.upcase } %> <%= 'CON ' + cents.to_s.rjust(2,'0') + '/100 DOLARES AMERICANOS' %>
