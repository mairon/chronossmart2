<h1 id="header">pedido_compras_presupuestos</h1>
<div class="acessos">
  <%= link_to t('NEW'), new_pedido_compras_presupuesto_path, :class => "button add" %>
</div>

<table class="head_grid">
  <tr>
            <td>Pedido <%= t('PURCHASE') %></td>
            <td>Presupuesto</td>
      </tr>
</table>

<div class="rolagem" dir="div">
  <table>
    <% @pedido_compras_presupuestos.each do |pedido_compras_presupuesto| %>
      <tr>
                  <td><%= pedido_compras_presupuesto.pedido_compra_id %></td>
                  <td><%= pedido_compras_presupuesto.presupuesto_id %></td>
                <td><%= link_to 'Editar', edit_pedido_compras_presupuesto_path(pedido_compras_presupuesto) %></td>
        <td><%= link_to t('DELETE'), pedido_compras_presupuesto, method: :delete, data: { confirm: 'Esta Seguro?' } %></td>
      </tr>
    <% end %>
  </table>
</div>

