<h1 id="header">produtos_tabela_precos</h1>
<div class="acessos">
  <%= link_to t('NEW'), new_produtos_tabela_preco_path, :class => "button add" %>
</div>

<table class="head_grid">
  <tr>
            <td>Produto</td>
            <td><%= t('UNIT') %></td>
      </tr>
</table>

<div class="rolagem" dir="div">
  <table>
    <% @produtos_tabela_precos.each do |produtos_tabela_preco| %>
      <tr>
                  <td><%= produtos_tabela_preco.produto_id %></td>
                  <td><%= produtos_tabela_preco.unidade_id %></td>
                <td><%= link_to 'Editar', edit_produtos_tabela_preco_path(produtos_tabela_preco) %></td>
        <td><%= link_to t('DELETE'), produtos_tabela_preco, method: :delete, data: { confirm: 'Esta Seguro?' } %></td>
      </tr>
    <% end %>
  </table>
</div>

