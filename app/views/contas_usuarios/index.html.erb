<h1 id="header">contas_usuarios</h1>
<div class="acessos">
  <%= link_to 'Nuevo', new_contas_usuario_path, :class => "button add" %>
</div>

<table class="head_grid">
  <tr>
            <td>Conta</t>
            <td>Usuario</t>
      </tr>
</table>

<div class="rolagem" dir="div">
  <table>
    <% @contas_usuarios.each do |contas_usuario| %>
      <tr>
                  <td><%= contas_usuario.conta_id %></td>
                  <td><%= contas_usuario.usuario_id %></td>
                <td><%= link_to 'Editar', edit_contas_usuario_path(contas_usuario) %></td>
        <td><%= link_to 'Borrar', contas_usuario, method: :delete, data: { confirm: 'Esta Seguro?' } %></td>
      </tr>
    <% end %>
  </table>
</div>

